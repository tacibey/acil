---
import Layout from '@layouts/Layout.astro';
import InnerHero from '@components/sections/InnerHero.astro';
import TeamGrid from '@components/sections/TeamGrid.astro';

const seoTitle = "Our Team";
const seoDescription = "Our team is made up of a diverse group of people who are passionate about building amazing websites with ease.";

const heroContent = {
    title: "Danışmanlarımız",
    description: "Uzman, enerjik ve yetkin AcilEmlakSat.com ekibi ile tanışın",
}

---

<Layout title={seoTitle} description={seoDescription}>
    <InnerHero content={heroContent}  />
    <TeamGrid padding="base" />
</Layout>
